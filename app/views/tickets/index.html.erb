  <div class="row">
    <div class="col-md">
      <h3>Trudy Fitness queue</h3>
    </div>
  </div>
  <table class="table">
    <thead class="thead-light">
      <tr class="products_header">
        <th>name</th>
        <th>email</th>
        <th>time</th>
      </tr>
    </thead>
    <tbody>
      <% @tickets.each do |p| %>
        <tr class="product">
          <td class="productname">
            <%= link_to "#{p.name}", ticket_path(p) %>
          </td>
          <td class="productname">
            <%= p.email %>
          </td>
          <td class="productprice">
            <%= p.created_at %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to('Create new ticket', new_ticket_path()) %>