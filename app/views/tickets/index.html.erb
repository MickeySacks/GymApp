<div class="row">
    <div class="col-md">
      <h3>Trudy Fitness queue</h3>
    </div>
</div>
<% unless @tickets.first.nil? %>
  <button>
    <%= link_to('Admit new gym user', @tickets.first, method: :delete) %> 
  </button>
<% end %>
<table class="table" id="queue">
  <thead class="thead-light">
    <tr>
      <th>Position</th>
      <th>Name</th>
      <th>Email</th>
      <th>Floor</th>
      <th>Time</th>
      <th>UserID</th>
      <th>Gym</th>
    </tr>
  </thead>
  <tbody>
    <% @tickets.each do |p| %>
      <tr>
        <td>
          <%= p.id %>
        </td>
        <td>
          <%= p.name %>
        </td>
        <td>
          <%= p.user.email %>
        </td>
        <td>
          <%= p.floor %>
        </td>
        <td>
          <%= p.created_at %>
        </td>
        <td>
          <%= p.user_id %>
        </td>
        <td>
          <%= p.gym.id %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>